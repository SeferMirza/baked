{
  "plugins": [
    {
      "name": "auth",
      "anonymousApiRoutes": [
        "cache-samples/scoped",
        "cache-samples/application",
        "authorization-samples/anonymous",
        "authentication-samples/login"
      ],
      "anonymousPageRoutes": [
        "^(?!.*auth).*$",
        "login"
      ],
      "loginPageRoute": "login",
      "refreshApiRoute": "authentication-samples/refresh"
    },
    {
      "name": "cacheApplication"
    },
    {
      "name": "cacheUser"
    },
    {
      "handlers": [
        {
          "statusCode": 401,
          "behavior": "Redirect",
          "behaviorArgument": {
            "type": "Computed",
            "composable": "useLoginRedirect",
            "args": []
          },
          "order": 0
        },
        {
          "statusCode": 400,
          "behavior": "Alert",
          "order": 0
        },
        {
          "behavior": "Page",
          "order": 0
        }
      ],
      "defaultAlertTitle": "Unexpected_Error",
      "defaultAlertMessage": "Please_contact_system_administrator",
      "name": "errorHandling"
    },
    {
      "supportedLanguages": [
        {
          "code": "en",
          "name": "English"
        },
        {
          "code": "tr",
          "name": "Turkish"
        }
      ],
      "name": "localization"
    }
  ],
  "error": {
    "type": "ErrorPage",
    "schema": {
      "errorInfos": {
        "403": {
          "title": "Access_Denied",
          "message": "You_do_not_have_the_permision_to_view_the_address_or_data_specified"
        },
        "404": {
          "title": "Page_Not_Found",
          "message": "The_page_you_want_to_view_is_etiher_deleted_or_outdated"
        },
        "500": {
          "title": "Unexpected_Error",
          "message": "Please_contact_system_administrator"
        }
      },
      "footerInfo": "If_you_cannot_reach_the_page_you_want_please_contact_the_system_administrator",
      "safeLinks": [
        {
          "type": "CardLink",
          "schema": {
            "route": "/",
            "icon": "pi pi-home",
            "title": "Home"
          }
        },
        {
          "type": "CardLink",
          "schema": {
            "route": "/cache",
            "icon": "pi pi-database",
            "title": "Cache"
          }
        },
        {
          "type": "CardLink",
          "schema": {
            "route": "/data-table",
            "icon": "pi pi-table",
            "title": "DataTable"
          }
        },
        {
          "type": "CardLink",
          "schema": {
            "route": "/report",
            "icon": "pi pi-file",
            "title": "Report"
          }
        },
        {
          "type": "CardLink",
          "schema": {
            "route": "/specs",
            "icon": "pi pi-list-check",
            "title": "Specs"
          }
        }
      ],
      "safeLinksMessage": "Try_the_links_from_the_menu_below_to_view_the_page_you_want_to_access"
    },
    "data": {
      "type": "Computed",
      "composable": "useNuxtError",
      "args": []
    }
  },
  "i18n": {
    "defaultLanguage": {
      "code": "en",
      "name": "English"
    },
    "supportedLanguages": [
      {
        "code": "en",
        "name": "English"
      },
      {
        "code": "tr",
        "name": "Turkish"
      }
    ]
  }
}