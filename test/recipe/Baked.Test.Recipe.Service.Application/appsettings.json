{
    "AllowedHosts": "*",
    "Authentication": {
        "FixedBearerToken": {
            "Description": {
                "additional": "Additional data for form post authentication"
            },

            "AdminUI": "token-admin-ui",
            "Jane": "token-jane",
            "John": "token-john",
            "Postman": "token-postman",
            "System": "token-system",

            "Authenticated": "token-authenticated",
            "BaseClaims": "token-base-claims",
            "GivenClaims": "token-given-claims",
            "GivenAndBaseClaims": "token-given-and-base-claims",
            "ClassClaims": "token-class-claims",
            "MethodOverClassClaims": "token-method-over-class-claims"
        }
    },
    "Communication": {
        "Http": {
            "Default": {
                "DefaultHeaders": {
                    "User-Agent": ".NET Http Client"
                }
            }
        }
    },
    "CorsOrigin": [
        "http://localhost:3000",
        "http://127.0.0.1:3000"
    ]
}
